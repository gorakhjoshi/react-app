{
  "name": "react-typescript-tailwind-starter",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint -c .eslintrc.cjs \"src/**/*.ts\" \"src/**/*.tsx\" --max-warnings=0",
    "lint:fix": "eslint --fix -c .eslintrc.cjs \"src/**/*.ts\" \"src/**/*.tsx\"",
    "format:fix": "pretty-quick --pattern \"src/**/*.{ts,tsx}\"",
    "format:check": "pretty-quick --staged --pattern \"src/**/*.{ts,tsx}\"",
    "pre-commit": "pnpm lint && pnpm format:check",
    "husky": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsonc-eslint-parser": "^2.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  },
  "license": "MIT"
}
